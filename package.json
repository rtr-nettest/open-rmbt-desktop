{
    "name": "open-rmbt-desktop",
    "productName": "RTR-Netztest",
    "version": "1.0.8",
    "description": "RTR-Netztest Desktop App",
    "main": "dist/main.js",
    "engines": {
        "node": ">=18"
    },
    "repository": "https://github.com/rtr-nettest/open-rmbt-desktop",
    "scripts": {
        "prepare:translations": "npx ts-node scripts/download-translations.ts",
        "build:all": "node scripts/set-package-name.js && webpack --mode production && cd src/ui && npm run build-for-packaging && shx cp -rf dist/ui/browser/* ../../dist",
        "start:cli": "webpack --mode development --config ./webpack.cli.config.js && cross-env DEV=true npx ts-node src/cli/cli.ts",
        "start:electron": "webpack --mode development && cross-env DEV=true electron dist/main.js",
        "start:ui": "(cd src/ui && npm run start)",
        "start:all": "node scripts/set-package-name.js && start-server-and-test start:ui http://localhost:4200/ start:electron",
        "electron": "electron",
        "test": "jest --coverage",
        "package": "npm run build:all && electron-forge package",
        "make": "npm run build:all && electron-forge make",
        "make:windows": "npm run build:all && electron-forge make --platform=win32 --arch=x64",
        "make:macos": "npm run build:all && cross-env MACOS=true electron-forge make --platform=darwin --arch=x64",
        "make:app-store": "npm run build:all && cross-env APP_STORE=true electron-forge make --platform=mas --arch=x64",
        "make:deb": "npm run build:all && cross-env DEB=true electron-forge make --platform=linux --arch=x64",
        "make:rpm": "npm run build:all && cross-env RPM=true electron-forge make --platform=linux --arch=x64",
        "prepare": "husky install"
    },
    "author": "RTR-Netztest",
    "license": "Apache-2.0",
    "devDependencies": {
        "@electron-forge/cli": "^7.4.0",
        "@electron-forge/maker-appx": "^7.4.0",
        "@electron-forge/maker-deb": "^7.4.0",
        "@electron-forge/maker-dmg": "^7.4.0",
        "@electron-forge/maker-pkg": "^7.4.0",
        "@electron-forge/maker-rpm": "^7.4.0",
        "@electron-forge/maker-squirrel": "^7.4.0",
        "@electron-forge/maker-zip": "^7.4.0",
        "@electron-forge/plugin-webpack": "^7.4.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.5.2",
        "@types/node-os-utils": "^1.3.4",
        "@types/semver": "^7.5.8",
        "@types/sql.js": "^1.4.9",
        "@types/stream-throttle": "^0.1.4",
        "@vercel/webpack-asset-relocator-loader": "^1.7.4",
        "copy-webpack-plugin": "^12.0.2",
        "cross-env": "^7.0.3",
        "dotenv": "^16.4.5",
        "dotenv-webpack": "^8.1.0",
        "electron": "^32.0.1",
        "husky": "^9.1.5",
        "jest": "^29.7.0",
        "node-loader": "^2.0.0",
        "node-noop": "^1.0.0",
        "plist": "^3.1.0",
        "shx": "^0.3.4",
        "start-server-and-test": "^2.0.5",
        "stream-throttle": "^0.1.3",
        "terser-webpack-plugin": "^5.3.10",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "xliff": "^6.2.1",
        "yargs": "^17.7.2"
    },
    "dependencies": {
        "axios": "^1.7.7",
        "dayjs": "^1.11.13",
        "electron-dl": "^4.0.0",
        "electron-squirrel-startup": "^1.0.1",
        "node-os-utils": "^1.3.7",
        "pino": "^9.4.0",
        "pino-pretty": "^11.2.2",
        "reflect-metadata": "^0.2.2",
        "sql.js": "^1.11.0",
        "uuidv4": "^6.2.13"
    },
    "gitInfo": {}
}