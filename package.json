{
  "name": "open-rmbt-desktop",
  "productName": "RTR-Netztest",
  "version": "0.2.13",
  "description": "RTR Desktop App",
  "main": "dist/main.js",
  "engines": {
    "node": ">=18"
  },
  "repository": "https://github.com/rtr-nettest/open-rmbt-desktop",
  "scripts": {
    "prepare:translations": "npx ts-node scripts/download-translations.ts",
    "build:all": "node scripts/set-package-name.js && webpack --mode production && cd src/ui && npm run build-for-packaging && shx cp -rf dist/ui/* ../../dist",
    "start:cli": "webpack --mode development --config ./webpack.cli.config.js && npx ts-node src/cli/cli.ts",
    "start:electron": "webpack --mode development && cross-env DEV=true electron dist/main.js",
    "start:ui": "(cd src/ui && npm run start)",
    "start:all": "start-server-and-test start:ui http://localhost:4200/ start:electron",
    "electron": "electron",
    "test": "jest --coverage",
    "package": "npm run build:all && electron-forge package",
    "make": "npm run build:all && electron-forge make",
    "make:windows": "npm run build:all && electron-forge make --platform=win32 --arch=x64",
    "make:macos": "npm run build:all && cross-env MACOS=true electron-forge make --platform=darwin --arch=x64",
    "make:app-store": "npm run build:all && cross-env APP_STORE=true electron-forge make --platform=mas --arch=x64",
    "make:deb": "npm run build:all && cross-env DEB=true electron-forge make --platform=linux --arch=x64",
    "make:rpm": "npm run build:all && cross-env RPM=true electron-forge make --platform=linux --arch=x64"
  },
  "author": "Specure",
  "license": "Apache-2.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.2.1",
    "@electron-forge/maker-appx": "^6.2.1",
    "@electron-forge/maker-deb": "^6.2.1",
    "@electron-forge/maker-dmg": "^6.2.1",
    "@electron-forge/maker-pkg": "^6.2.1",
    "@electron-forge/maker-rpm": "^6.2.1",
    "@electron-forge/maker-squirrel": "^6.3.0",
    "@electron-forge/maker-zip": "^6.2.1",
    "@electron-forge/plugin-webpack": "^6.2.1",
    "@tsconfig/node18": "^2.0.1",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-os-utils": "^1.3.1",
    "@types/semver": "^7.5.0",
    "@types/sql.js": "^1.4.4",
    "@types/stream-throttle": "^0.1.1",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "electron": "^25.1.1",
    "jest": "^29.5.0",
    "node-loader": "^2.0.0",
    "node-noop": "^1.0.0",
    "plist": "^3.1.0",
    "shx": "^0.3.4",
    "start-server-and-test": "^2.0.0",
    "stream-throttle": "^0.1.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack": "^5.87.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dayjs": "^1.11.8",
    "dotenv": "^16.3.1",
    "electron-dl": "^3.5.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "event-loop-lag": "^1.4.0",
    "node-os-utils": "^1.3.7",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "sql.js": "^1.8.0",
    "update-electron-app": "^2.0.1",
    "uuidv4": "^6.2.13",
    "xliff": "^6.1.0"
  }
}